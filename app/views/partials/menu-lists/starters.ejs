<!-- Parameters: starters -->
<% if(starters.length) { %>
    <ul class="list-group menu-list-group starters <% if(!menu.mains.length) { %>mb-0<% } %>">
        <% starters.forEach(function (starter) { %>
            <li class="list-group-item">
                <%= starter.name %>
                <% if(starter.allergens) { %>
                    <span class="allergens"><%= starter.allergens %></span>
                <% } %>
            </li>
        <% }); %>
    </ul>
<% } %>