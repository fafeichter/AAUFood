<!-- Parameters: restaurant, menuState -->
<% switch (menuState) {
case menuStateHelper.MenuState.OnBreak: %>
<%- include('on-break', {restaurantId}) %>
<% break;
case menuStateHelper.MenuState.Closed: %>
<%- include('closed', {restaurantId}) %>
<% break;
case menuStateHelper.MenuState.NoMenu: %>
<%- include('no-menu', {restaurantId}) %>
<% break;
case menuStateHelper.MenuState.Outdated: %>
<%- include('outdated', {restaurantId, restaurantFriendlyUrl}) %>
<% break;
case menuStateHelper.MenuState.ScrapingNotImplemented: %>
<%- include('not-implemented', {restaurantId, restaurantFriendlyUrl}) %>
<% break; %>
case menuStateHelper.MenuState.Error: %>
<%- include('error', {restaurantId, restaurantFriendlyUrl}) %>
<% break; %>
<% } %>